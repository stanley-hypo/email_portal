{
  "name": "email_portal",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:generate": "drizzle-kit generate",
    "db:drop": "drizzle-kit drop",
    "db:migrate": "tsx src/db/migrate.ts",
    "db:refresh": "tsx src/db/drop-prompt.ts",
    "db:update": "npm run db:generate && npm run db:migrate",
    "db:seed": "tsx src/db/seed-prompt.ts",
    "test": "jest --watch",
    "test:ci": "jest --ci",
    "refresh-token": "tsx jobs/refresh_token.js",
    "typecheck": "tsc --noEmit",
    "worker": "ts-node src/workers/start.ts"
  },
  "dependencies": {
    "@mantine/core": "^7.17.3",
    "@mantine/form": "^7.17.3",
    "@mantine/hooks": "^7.17.3",
    "@mantine/modals": "^7.17.3",
    "@tabler/icons-react": "^3.31.0",
    "@types/crypto-js": "^4.2.2",
    "@types/nodemailer": "^6.4.17",
    "bullmq": "^5.46.0",
    "crypto-js": "^4.2.0",
    "next": "15.2.4",
    "nodemailer": "^6.10.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
